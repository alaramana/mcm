<!--entering the data for creating Complaint Types Details-->
<h1><%= I18n.t 'enter_complaint_type_details' %><br>Enter Complaint Types Details:</h1>
<%= form_tag :action => 'create' %>
	<table>
	<tr>
		<td><%= I18n.t "complaint_type" %>:<br><label for="complaint_type">Complaint Type</label>:*</td>
		<td><%= text_field 'complainttype', 'complaint_type',:class=>'title' %></td>
	</tr>	
	<tr>
		<td><%= I18n.t "description" %>:<br><label for="description">Description</label>:</td>
		<td><%= text_area 'complainttype', 'description' %></td>
	</tr>
	<tr>
		<td></td>
		<td><%= I18n.t "save" %><br><%= submit_tag "Create" %></td>
	</tr>
	</table>
<%= form_tag %>


