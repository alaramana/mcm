<!--entering the data for creating Property Type Details-->
<h1><%= I18n.t "enter_property_type_details" %>:<br>Enter Property Type Details:</h1>
<%= form_tag :action => 'create' %>
	<table>
	<tr>
		<td><label for="name"><%= I18n.t "property_type" %>:<br> Property Type</label>:* </td>
		<td><%= text_field 'propertytype', 'name',:class=>'title' %></td>
	</tr>
	<tr>
		<td><label for="description"><%= I18n.t "description" %>:<br>Description</label>:</td>
		<td><%= text_area 'propertytype', 'description' %></td>
	</tr>
	<tr>
		<td></td>
		<td><%= I18n.t "save" %><br><%= submit_tag "Create" %></td>
	</tr>
	</table>
<%= form_tag %>


