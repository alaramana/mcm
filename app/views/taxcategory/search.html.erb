<!-- Search about  Tax Categories -->
<h1><%= I18n.t "search" %>:<br>Search Results </h1>
<% if @taxcategories.blank? %>
<p><%= I18n.t "tax_category_details_no" %>:<br>There are no Tax Categories currently in the system.</p>
<% else %>
	<p><%= I18n.t "tax_category_details" %>:<br>These are the current tax catefories:</p>
	<table>
	<tr><td><%= I18n.t "tax_category" %><br>Category</td<td><%= I18n.t "edit" %><br>Edit</td></tr>
	<% @taxcategories.each do |c| %>
	<tr>
		<td><%= link_to c.category, {:action => 'show', :id => c.id} -%></td>
		<td><%= link_to 'Edit', {:action => 'edit', :id => c.id} %></b></td>
	</tr>
	<% end %>
	</table>
<% end %>
<%= link_to I18n.t "back" ,{:action => 'list'}%><br> <%= link_to 'Back', {:action => 'list'} %>
