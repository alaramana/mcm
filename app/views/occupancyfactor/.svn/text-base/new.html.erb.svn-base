<!--entering the data for creating Occupancy Factor Details--> 
<h1><%= I18n.t "enter_occupancy_factor_details" %>:<br>Enter Occupancy Factor Details:
<%= form_tag :action => 'create' %>
	<table>
	<tr>
		<td><label for="name"><%= I18n.t "occupancy_factor" %>:<br> Occupancy Factor Name</label>:* </td>
		<td><%= text_field 'occupancyfactor', 'name',:class=>'title' %></td>
	</tr>
	<tr>
		<td><label for="name"><%= I18n.t "value" %>:<br> Value</label>:* </td>
		<td><%= text_field 'occupancyfactor', 'factor',:class=>'title' %></td>
	</tr>
	<tr>
		<td><label for="description"> <%= I18n.t "description" %>:<br> Description</label>:</td>
		<td><%= text_area 'occupancyfactor', 'description' %></td>
	</tr>
	<tr>
		<td></td>
		<td> <%= I18n.t "save" %>:<br> <%= submit_tag "Create" %></td>
	</tr>
	</table>
<%= form_tag %>


