<!-- Search about  Occupancy Factor-->
<h1><%= I18n.t "search" %>:<br>Search Results </h1>
	<% if @occupancyfactors.blank? %>
	<p><%= I18n.t "occupancy_factor_details_no" %>:<br>There are no Occupancy Factor currently in the system.</p>
<% else %>
	<p><%= I18n.t "occupancy_factor_details" %>:<br>These are the current Occupancy Factor:</p>
	<table>
	<tr>
		<td><%= I18n.t "name" %><br>Name</td>
		<td><%= I18n.t "occupancy_factor" %><br>Occupancy Factor</td>
		<td><%= I18n.t "edit" %><br>Edit</td>
	</tr>
	<% @occupancyfactors.each do |c| %>
	<tr>
		<td><%= link_to c.name, {:action => 'show', :id => c.id} -%></td>
		<td><%= c.factor -%> </td>
		<td><%= link_to 'Edit', {:action => 'edit', :id => c.id} %></td>
	</tr>
	<% end %>
	</table>
<% end %>
<%= will_paginate @occupancyfactors %>	
<%= link_to I18n.t "back" ,{:action => 'list'}%><br> <%= link_to 'Back', {:action => 'list'} %>
