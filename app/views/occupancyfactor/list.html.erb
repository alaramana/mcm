<!--Listing all the Occupancy Factor Details-->
<% if @occupancyfactors.blank? %>
	<p><%= I18n.t "occupancy_factor_details_no" %>:<br>There are no  Occupancy Factor currently in the system.</p>
<% else %>

<!--Start of search text box -->
<%= form_tag :action => 'search' %>
	<table>
	<tr>
		<td><%= I18n.t "search" %>:<br>Search: </td>
		<td>
<%= text_field_tag :query ,@query ||="Enter Occupancy Factor " ,:class=>'title',:onfocus=>"javascript: this.value=''",:onblur=>"javascript: if(this.value=='') this.value='Enter Occupancy Factor'" %></td>
		<td><%= I18n.t "search" %>:<br><%= submit_tag "Occupancy Factor" %></td>
	</tr>
	</table>
<%= form_tag %>
<!--end of search text box -- >
<br>
	<p><%= I18n.t "occupancy_factor_details" %>:<br>These are the current Occupancy Factor:</p>
	<table>
	<tr>
		<td><%= I18n.t "occupancy_factor" %><br>Occupancy</td>
		<td><%= I18n.t "edit" %><br>Edit</td>
	</tr>
	<% @occupancyfactors.each do |c| %>
	<tr>
		<td><%= link_to c.name, {:action => 'show', :id => c.id} -%></td>
		<td><%= link_to 'Edit', {:action => 'edit', :id => c.id} %></td>
	</tr>
	<% end %>
	</table>
<% end %>
<%= will_paginate @occupancyfactors %>
<%= link_to I18n.t "add_new" , {:action => 'new' } %><br><%= link_to "Add new", {:action => 'new' }%>



