<!-- Search about Genders-->
<h1><%= I18n.t "search" %>:<br>Search Results </h1>
	<% if @genders.blank? %>
	<p><%= I18n.t "gender_details_no" %>:<br>There are no Genders currently in the system.</p>
<% else %>
	<p><%= I18n.t "gender_details" %>:<br>These are the current genders:</p>
	<table>
	<tr>
		<td><%= I18n.t "gender" %><br>Gender</td>
		<td><%= I18n.t "edit" %><br>Edit</td>
	</tr>
	<% @genders.each do |c| %>
	<tr>
		<td><%= link_to c.name, {:action => 'show', :id => c.id} -%></td>
		<td><%= link_to 'Edit', {:action => 'edit', :id => c.id} %></td>	
	</tr>
	<% end %>
	</table>
<% end %>
<%= will_paginate @genders %>	
<%= link_to I18n.t "back" ,{:action => 'list'}%><br> <%= link_to 'Back', {:action => 'list'} %>
