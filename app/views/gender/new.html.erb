<!--entering the data for Gender Details-->
<h1><%= I18n.t "enter_gender_details" %>:<br>Enter Gender Details:</h1>
<%= form_tag :action => 'create' %>
	<table>
	<tr>
		<td><label for="name"><%= I18n.t "gender" %>:<br>Gender</label>: </td>
		<td><%= text_field 'gender', 'name',:class=>'title' %></td>
	</tr>
	<tr>
		<td><label for="description"><%= I18n.t "description" %>:<br>Description</label>:</td>
		<td><%= text_area 'gender', 'description' %></td>
	</tr>
	<tr>
		<td></td>
		<td><%= I18n.t "save" %>:<br><%= submit_tag "Create" %></td>
	</tr>
	</table>
<%= form_tag %>


