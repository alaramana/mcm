<!--Listing all DA-->
<% if @das.blank? %>
	<p><%= I18n.t "da_amount_details_no" %><br>There are no DA currently in the system.</p>
<% else %>
	<p><%= I18n.t "da_amount_details" %><br>These are the current DA:</p>
	<table>
	<tr>
		<td><%= I18n.t "da_amount" %><br>DA Percentage</td>
		<td><%= I18n.t "edit" %><br>Edit</td>
	</tr>
	<% @das.each do |c| %>
	<tr>
		<td><%= link_to c.da_amount, {:action => 'show', :id => c.id} -%></td>
		<td><%= link_to 'Edit', {:action => 'edit', :id => c.id} %></td>
	</tr>
	<% end %>
	</table>
<% end %>
<%= will_paginate @das %>
<%= link_to I18n.t "add_new" , {:action => 'new' } %><br><%= link_to "Add new", {:action => 'new' }%>



