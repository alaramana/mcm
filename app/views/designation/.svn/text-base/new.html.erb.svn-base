<!--entering the data for creating Designation Details-->
<h1> <%= I18n.t "enter_designation_details" %>:<br>Enter Designation Details:<h1>
<%= form_tag :action => 'create' %>
	<table>
	<tr>
		<td><label for="name"><%= I18n.t "designation" %>:<br> Designation</label>:* </td>
		<td><%= text_field 'designation', 'name',:class=>'title' %></td>
	</tr>
	<tr>
		<td><label for="description"><%= I18n.t "description" %>:<br>Description</label>:</td>
		<td><%= text_field 'designation', 'description' %></td>
	</tr>
	<tr>
		<td></td>
		<td><%= I18n.t "save" %>:<br><%= submit_tag "Create" %></td>
	</tr>
	</table>
<%= form_tag %>
