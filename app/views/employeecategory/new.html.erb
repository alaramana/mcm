<!--entering the data for creating Employee Type Details-->
<h1><%= I18n.t "enter_employee_type_details" %>:<br>Enter Employee Category Details:</h1>
<%= form_tag :action => 'create' %>
	<table>
	<tr>
		<td><label for="name"><%= I18n.t "employee_type" %>:<br>Employee Category</label>:* </td>
		<td><%= text_field 'employeecategory', 'name',:class=>'title' %> </td>
	</tr>
	<tr>
		<td><label for="description"> <%= I18n.t "description" %>:<br>Description</label>:</td>
		<td><%= text_field 'employeecategory', 'description' %></td>
	</tr>
	<tr>
		<td></td>
		<td><%= I18n.t "save" %><br><%= submit_tag "Create" %></td>
	</tr>
	</table>
<%= form_tag %>


