<!-- Search about Rental Types-->
<h1><%= I18n.t "search" %>:<br>Search Results </h1>
	<% if @rentaltypes.blank? %>
	<p><%= I18n.t "rental_type_details_no" %><br>There are no Rental Types with that Tag</p>
<% else %>
	<p><%= I18n.t "rental_type_details" %><br>These are the current Rental Types:</p>
	<table>
	<tr>
		<td><%= I18n.t "rental_type" %><br>License Type</td>		
		<td><%= I18n.t "edit" %><br>Edit</td>
	</tr>
	<% @rentaltypes.each do |c| %>
	<tr>
		<td><%= link_to c.rental_type, {:action => 'show', :id => c.id} -%></td>		
		<td><%= link_to 'Edit', {:action => 'edit', :id => c.id} %></td>
	</tr>
	<% end %>
	</table>
<% end %>
<%= will_paginate @rentaltypes %>
<%= I18n.t "back" %><br> <%= link_to 'Back', {:action => 'list'} %>
