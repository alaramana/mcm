<!--entering the data for creating Structure Factor Details-->
<h1><%= I18n.t "enter_structure_factor_details" %>:<br>Enter Structure Factor Details:</h1>
<%= form_tag :action => 'create' %>
	<table>
	<tr>
		<td><label for="name"> <%= I18n.t "structure_factor" %>:<br>Structure Factor Name</label>:* </td>
		<td><%= text_field 'structurefactor', 'name',:class=>'title' %> </td>
	</tr>
	<tr>
		<td><label for="name"> <%= I18n.t "value" %>:<br> Value</label>:* </td>
		<td><%= text_field 'structurefactor', 'factor',:class=>'title' %></td>
	</tr>
	<tr>
		<td><label for="description"><%= I18n.t "description" %>:<br>Description</label>:</td>
		<td><%= text_area 'structurefactor', 'description' %></td>
	</tr>
	<tr>
		<td></td>
		<td><%= I18n.t "save" %>:<br><%= submit_tag "Create" %></td>
	</tr>
	</table>
<%= form_tag %>


