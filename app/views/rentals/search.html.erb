<!-- Search about rentals-->
<h1><%= I18n.t "search" %>:<br>Search Results </h1>
	<% if @rentals.blank? %>
	<p><%= I18n.t "rental_registration_details_no" %><br>There are no Rentals with that Tag .</p>
<% else %>
	<p><%= I18n.t "rental_registration_details" %><br>These are the current Rentals:</p>
	<table>
	<tr>
		<td><%= I18n.t "rental_number" %><br>Rental Number</td>
		<td><%= I18n.t "rental_type" %><br>Rental Type</td>		
		<td><%= I18n.t "edit" %><br>Edit</td>
	</tr>
	<% @rentals.each do |c| %>
	<tr>
		<td><%= link_to c.rental_number, {:action => 'show', :id => c.id} -%></td>
		<td><%= c.rentaltype.lic_type -%></td>				
		<td><%= link_to 'Edit', {:action => 'edit', :id => c.id} %></td>
	</tr>
	<% end %>
	</table>
<% end %>
<%= will_paginate @rentals %>
<%= I18n.t "back" %><br> <%= link_to 'Back', {:action => 'list'} %>
