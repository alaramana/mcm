<!-- Search about License Types-->
<h1><%= I18n.t "search" %>:<br>Search Results </h1>
	<% if @licensetypes.blank? %>
	<p><%= I18n.t 'license_type_details_no' %><br>There are no License Types with that Tag</p>
<% else %>
	<p><%= I18n.t 'license_type_details' %><br>These are the current License Types:</p>
	<table>
	<tr>
		<td><%= I18n.t 'license_type' %><br>License Type</td>		
		<td><%= I18n.t 'edit' %><br>Edit</td>
	</tr>
	<% @licensetypes.each do |c| %>
	<tr>
		<td><%= link_to c.lic_type, {:action => 'show', :id => c.id} -%></td>		
		<td><%= link_to 'Edit', {:action => 'edit', :id => c.id} %></td>
	</tr>
	<% end %>
	</table>
<% end %>
<%= will_paginate @licensetypes %>
<%= I18n.t "back" %><br> <%= link_to 'Back', {:action => 'list'} %>
