<!-- Search about Licenses-->
<h1><%= I18n.t "search" %>:<br>Search Results </h1>
	<% if @licenses.blank? %>
	<p><%= I18n.t "license_registration_details_no" %><br>There are no Licenses with that Tag .</p>
<% else %>
	<p><%= I18n.t "license_registration_details" %><br>These are the current Licenses:</p>
	<table>
	<tr>
		<td><%= I18n.t 'license_number' %><br>License Number</td>
		<td><%= I18n.t 'license_type' %><br>License Type</td>				
		<td><%= I18n.t "edit" %><br>Edit</td>
	</tr>
	<% @licenses.each do |c| %>
	<tr>
		<td><%= link_to c.license_number, {:action => 'show', :id => c.id} -%></td>		<td><%= c.licensetype.lic_type -%></td>				
		<td><%= link_to 'Edit', {:action => 'edit', :id => c.id} %></td>
	</tr>
	<% end %>
	</table>
<% end %>
<%= will_paginate @licenses %>
<%= I18n.t "back" %><br> <%= link_to 'Back', {:action => 'list'} %>
