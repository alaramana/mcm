<!-- Search about Item categorys-->
<h1><%= I18n.t "search" %>:<br>Search Results </h1>
<% if @itemcategories.blank? %>
 	<p>There are no Item categories currently in the system:</p>
<% else %>
	<p>These are the current Item categories:</p>
	<table>
	<tr>
		<td><%= I18n.t "name" %><br>Item Category</td<td><%= I18n.t "edit" %><br>Edit</td>
	</tr>
	<% @itemcategories.each do |c| %>
	
	<tr>
		<td><%= link_to c.category_name, {:action => 'show', :id => c.id} -%></td>
		<td><%= link_to 'Edit', {:action => 'edit', :id => c.id} %></td>
	</tr>
	<% end %>
	</table>
<% end %>
<%= will_paginate @itemcategories %>
<%= link_to I18n.t "back" ,{:action => 'list'}%><br> <%= link_to 'Back', {:action => 'list'} %>
