<!-- Search about  Tax Details-->
<h1><%= I18n.t "search" %>:<br>Search Results </h1>
	<% if @taxes.blank? %>
	<p><%= I18n.t 'service_tax_details_no' %><br>There are no  Tax Details currently in the system.</p>
<% else %>
	<p><%= I18n.t 'service_tax_details' %><br>These are the current  Tax Details:</p>
	<table>
	<tr> 
	     <td><%= I18n.t "tax" %><br>Tax </td>
	     <td><%=  I18n.t "amount" %><br>Amount </td>
	     <td><%= I18n.t "edit" %><br>Edit </td>
	</tr>
	<% @taxes.each do |c| %>
	
	<tr>
		<td><%= link_to c.name, {:action => 'show', :id => c.id} -%></td>
		<td><%=  c.amount -%></td>
		<td><%= link_to 'Edit', {:action => 'edit', :id => c.id} %></td>
	</tr>
	<% end %>
	</table>
<% end %>
<%= will_paginate @taxes %>	
<%= link_to I18n.t "back" ,{:action => 'list'}%><br> <%= link_to 'Back', {:action => 'list'} %>
