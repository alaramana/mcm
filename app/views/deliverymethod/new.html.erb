<!--entering the data for creating Delivery Method Details-->
<h1><%= I18n.t 'enter_delivery_method_details' %>:<br>Enter Delivery Method Details:</h1>
<%= form_tag :action => 'create' %>
	<table>
	<tr>
		<td><label for="name"><%= I18n.t 'delivery_method' %>:<br>Delivery Method</label>:* </td>
		<td><%= text_field 'deliverymethod', 'name',:class=>'title' %></td>
	</tr>
	<tr>
		<td><label for="description"><%= I18n.t 'description' %>:<br>Description</label>:</td>
		<td><%= text_field 'deliverymethod', 'description' %></td>
	</tr>
	<tr>
		<td></td>
		<td><%= I18n.t 'save' %>:<br><%= submit_tag "Create" %></td>
	</tr>
	</table>
<%= form_tag %>

<%= I18n.t 'back' %>:<br><%= link_to 'Back', {:action => 'list' } %>
